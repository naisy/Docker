RUN apt-get update \
    && apt-get install -y \
      vim \
      less \
      htop \
      unzip \
      locate \
      git \
      curl \
      wget \
      arp-scan \
      v4l-utils \
      gnupg2 \
      locales \
      lsb-release \
      net-tools \
      colortest \
      ssh \
      iputils-ping \
      tree \
    && apt autoremove -y \
    && rm -rf /var/lib/apt/lists/*


# 足りないライブラリを探すのは、cv2をインストールしてから。
# ここではすでに判明している足りないライブラリをインストールする
RUN sudo apt-get update \
    && sudo apt-get install --reinstall -y \
      libmp3lame0 \
      libxvidcore4 \
      libflac8 \
    && sudo apt autoremove -y \
    && sudo rm -rf /var/lib/apt/lists/*


