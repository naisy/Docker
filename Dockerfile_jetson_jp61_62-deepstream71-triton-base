########################################
# fix broken libs
########################################
# この関数は廃止された。残っているのは壊れているから。
# strings /usr/lib/aarch64-linux-gnu/libgobject-2.0.so.0 | grep g_bookmark
# strings /usr/lib/aarch64-linux-gnu/libgio-2.0.so.0 | grep g_hash_table_get_values_as_ptr_array

RUN sudo rm /usr/lib/aarch64-linux-gnu/libgobject-2.0.so.0.* \
