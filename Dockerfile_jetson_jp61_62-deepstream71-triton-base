########################################
# Base OS repogitory
########################################
# https://catalog.ngc.nvidia.com/orgs/nvidia/containers/deepstream-l4t
# https://catalog.ngc.nvidia.com/containers?filters=&orderBy=scoreDESC&query=l4t-cuda&page=&pageSize=

# JetPack 6.2/L4T R36.4.3
# JetPack 6.1/L4T R36.4
# CUDA 12.6, TensorRT 10.3, cuDNN 9.3, VPI 3.2, DLA 3.1, and DLFW 24.0

#nvcr.io/nvidia/l4t-jetpack:r36.4.0
#nvcr.io/nvidia/deepstream-l4t:7.1-triton-multiarch

ARG BASE_IMAGE=nvcr.io/nvidia/deepstream-l4t:7.1-triton-multiarch
FROM ${BASE_IMAGE}

########################################
# ENV
########################################
ENV DEBIAN_FRONTEND=noninteractive

########################################
# change the locale from POSIX to UTF-8
########################################
RUN locale-gen en_US en_US.UTF-8 && update-locale LC_ALL=en_US.UTF-8 LANG=en_US.UTF-8
ENV LANG=en_US.UTF-8

########################################
# apt fix
########################################
# Because sometime the repository is 404, use with --fix-missing option.
RUN rm -rf /var/lib/apt/lists/* \
    && apt-get --fix-missing update \
    && apt-get install -y apt-utils \
    && apt autoremove -y \
    && rm -rf /var/lib/apt/lists/*


########################################
# L4T R36.4 Repogitory
########################################
RUN printf 'deb [signed-by=/etc/apt/keyrings/nvidia-jetson.gpg] https://repo.download.nvidia.com/jetson/common r36.4 main\n\
deb [signed-by=/etc/apt/keyrings/nvidia-jetson.gpg] https://repo.download.nvidia.com/jetson/t234 r36.4 main\n\
deb [signed-by=/etc/apt/keyrings/nvidia-jetson.gpg] https://repo.download.nvidia.com/jetson/ffmpeg r36.4 main\n'\
> /etc/apt/sources.list.d/nvidia-jetpack-r36.list

RUN printf 'Package: *\n\
Pin: origin "repo.download.nvidia.com"\n\
Pin-Priority: 1001\n'\
> /etc/apt/preferences.d/nvidia-jetson.pref

########################################
# GPT Key
########################################
RUN mkdir -p /etc/apt/keyrings \
    && curl -fsSL https://repo.download.nvidia.com/jetson/jetson-ota-public.asc | \
      gpg --dearmor -o /etc/apt/keyrings/nvidia-jetson.gpg


########################################
# change group ids
########################################
# JetPack 6.2は、hostのjetsonユーザにinputグループのアクセス権限がないので、USB Joystickにアクセスできない。
# 権限を付与しておく必要がある
# jetson@ubuntu:~$ groups
# jetson adm cdrom sudo audio dip video plugdev render i2c lpadmin gdm sambashare weston-launch gpio
# sudo usermod -aG input jetson
# 再ログイン後にでグループ権限を確認できる

# triton-serverのユーザIDとグループを 1000 -> 2000 に変更 (hostと同じjetsonユーザが利用する)
# nvidia-dcgmのユーザIDとグループを 999 -> 3000 に変更 (hostと同じgpioグループが利用する)

# nvcr.io/nvidia/deepstream-l4t:7.1-triton-multiarch
# Add gpio debug trusty crypto weston-launch colord group
# Change to the same group id as host
# groupname   container -> host
# _ssh 101 -> 105
# triton-server 1000 -> 2000
# rdma 102 -> 113
# systemd-journal 103 -> 108
# systemd-network 104 -> 109
# systemd-resolve 105 -> 110
# messagebus 106 -> 111
# nvidia-dcgm 999 -> 3000
# syslog 107 -> 114
# i2c -> 116

# ls -l /dev/
# cat /etc/group
RUN usermod -u 2000 triton-server \
    && groupmod -g 2000 triton-server \
    && usermod -u 3000 nvidia-dcgm \
    && groupmod -g 3000 nvidia-dcgm \
    && chown -h nvidia-dcgm /var/log/nvidia-dcgm \
    && chgrp -h nvidia-dcgm /var/log/nvidia-dcgm \
    && groupmod -g 111 messagebus \
    && groupmod -g 110 systemd-resolve \
    && groupmod -g 109 systemd-network \
    && groupmod -g 108 systemd-journal \
    && groupmod -g 105 _ssh \
    && groupmod -g 113 rdma \
    && groupmod -g 114 syslog \
    && groupadd -f -r input \
    && groupmod -g 101 input \
    && groupadd -f -r sgx \
    && groupmod -g 102 sgx \
    && groupadd -f -r kvm \
    && groupmod -g 103 kvm \
    && groupadd -f -r render \
    && groupmod -g 104 render \
    && groupadd -f -r dhcpd \
    && groupmod -g 106 dhcpd \
    && groupadd -f -r netdev \
    && groupmod -g 107 netdev \
    && groupadd -f -r systemd-timesync \
    && groupmod -g 112 systemd-timesync \
    && groupadd -f -r bluetooth \
    && groupmod -g 115 bluetooth \
    && groupadd -f -r i2c \
    && groupmod -g 116 i2c \
    && groupadd -f -r rtkit \
    && groupmod -g 117 rtkit \
    && groupadd -f -r tss \
    && groupmod -g 118 tss \
    && groupadd -f -r crontab \
    && groupmod -g 119 crontab \
    && groupadd -f -r lpadmin \
    && groupmod -g 120 lpadmin \
    && groupadd -f -r ssl-cert \
    && groupmod -g 121 ssl-cert \
    && groupadd -f -r systemd-oom \
    && groupmod -g 122 systemd-oom \
    && groupadd -f -r uuidd \
    && groupmod -g 123 uuidd \
    && groupadd -f -r geoclue \
    && groupmod -g 124 geoclue \
    && groupadd -f -r pulse \
    && groupmod -g 125 pulse \
    && groupadd -f -r pulse-access \
    && groupmod -g 126 pulse-access \
    && groupadd -f -r scanner \
    && groupmod -g 127 scanner \
    && groupadd -f -r avahi-autoipd \
    && groupmod -g 128 avahi-autoipd \
    && groupadd -f -r saned \
    && groupmod -g 129 saned \
    && groupadd -f -r nm-openvpn \
    && groupmod -g 130 nm-openvpn \
    && groupadd -f -r colord \
    && groupmod -g 131 colord \
    && groupadd -f -r avahi \
    && groupmod -g 132 avahi \
    && groupadd -f -r sssd \
    && groupmod -g 133 sssd \
    && groupadd -f -r fwupd-refresh \
    && groupmod -g 134 fwupd-refresh \
    && groupadd -f -r gdm \
    && groupmod -g 135 gdm \
    && groupadd -f -r sambashare \
    && groupmod -g 136 sambashare \
    && groupadd -f -r logusers \
    && groupmod -g 150 logusers \
    && groupadd -f -r debug \
    && groupmod -g 995 debug \
    && groupadd -f -r weston-launch \
    && groupmod -g 996 weston-launch \
    && groupadd -f -r crypto \
    && groupmod -g 997 crypto \
    && groupadd -f -r trusty \
    && groupmod -g 998 trusty \
    && groupadd -f -r gpio \
    && groupmod -g 999 gpio


########################################
# XAuthority
########################################
RUN apt-get update \
    && apt-get install -y \
      sudo \
      xauth \
      xterm \
    && apt autoremove -y \
    && rm -rf /var/lib/apt/lists/*
#Add new sudo usergrou
ARG USERNAME=jetson
RUN useradd -m $USERNAME && \
        echo "$USERNAME:$USERNAME" | chpasswd && \
        usermod --shell /bin/bash $USERNAME && \
        usermod -aG sudo,adm,cdrom,audio,dip,video,plugdev,render,i2c,lpadmin,gpio,input $USERNAME && \
        echo "$USERNAME ALL=(ALL) NOPASSWD:ALL" >> /etc/sudoers.d/$USERNAME && \
        chmod 0440 /etc/sudoers.d/$USERNAME && \
        # Replace 1000 with your user/group id
        usermod  --uid 1000 $USERNAME && \
        groupmod --gid 1000 $USERNAME && \
        chown -R $USERNAME:$USERNAME /home/$USERNAME


########################################
# bash setting
########################################
# sed
# escape characters \'$.*/[]^
# 1. Write the regex between single quotes.
# 2. \ -> \\
# 3. ' -> '\''
# 4. Put a backslash before $.*/[]^ and only those characters.


####################
# USER
####################
# before
# #force_color_prompt=yes
# after
# force_color_prompt=yes

# deepstream-l4t:7.1-triton-multiarch
# before
#    PS1='${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w\[\033[00m\]\$ '
# after
#    PS1='${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\[\033[01;31m\]docker-\h\[\033[00m\]:\[\033[01;35m\]\w\[\033[00m\]\$ '

# before
# alias ls='ls --color=auto'
# after
# alias ls='ls -asiF --color=auto'

RUN sed -i 's/#force_color_prompt=yes/force_color_prompt=yes/g' /home/$USERNAME/.bashrc \
&& sed -i 's/PS1='\''\${debian_chroot:+(\$debian_chroot)}\\\[\\033\[01;32m\\\]\\u@\\h\\\[\\033\[00m\\\]:\\\[\\033\[01;34m\\\]\\w\\\[\\033\[00m\\\]\\\$ '\''/PS1='\''\${debian_chroot:+(\$debian_chroot)}\\\[\\033\[01;32m\\\]\\u@\\\[\\033\[01;31m\\\]docker-\\h\\\[\\033\[00m\\\]:\\\[\\033\[01;35m\\\]\\w\\\[\\033\[00m\\\]\\\$ '\''/g' /home/$USERNAME/.bashrc \
&& sed -i 's/alias ls='\''ls --color=auto'\''/alias ls='\''ls -asiF --color=auto'\''/g' /home/$USERNAME/.bashrc \
&& echo "alias xterm='xterm -fa \"Monospace\" -fs 10 -fg white -bg black'" >> /home/$USERNAME/.bashrc \
&& echo 'export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda-12.6/targets/aarch64-linux/lib:/usr/local/lib' >> /home/$USERNAME/.bashrc \
&& echo 'export LANG=en_US.UTF-8' >> /home/$USERNAME/.bashrc \
&& echo 'export NO_AT_BRIDGE=1' >> /home/$USERNAME/.bashrc \
&& echo 'export PATH=$PATH:/usr/src/tensorrt/bin' >> /home/$USERNAME/.bashrc \
&& echo 'cd' >> /home/$USERNAME/.bashrc \
&& echo "complete -d cd" >> /home/$USERNAME/.bashrc


####################
# ROOT
####################
# before
#    xterm-color) color_prompt=yes;;
# after
#    xterm-color|*-256color) color_prompt=yes;;

# before
#    PS1='${debian_chroot:+($debian_chroot)}\[\033[01;32m\]\u@\h\[\033[00m\]:\[\033[01;34m\]\w\[\033[00m\]\$ '
# after
#    PS1='${debian_chroot:+($debian_chroot)}\[\033[01;37m\]\u@\[\033[01;31m\]docker-\h\[\033[00m\]:\[\033[01;35m\]\w\[\033[00m\]\$ '

# before
# alias ls='ls --color=auto'
# after
# alias ls='ls -asiF --color=auto'

RUN sed -i 's/xterm-color) color_prompt=yes;;/xterm-color|\*-256color) color_prompt=yes;;/g' /root/.bashrc \
&& sed -i 's/#force_color_prompt=yes/force_color_prompt=yes/g' /root/.bashrc \
&& sed -i 's/PS1='\''\${debian_chroot:+(\$debian_chroot)}\\\[\\033\[01;32m\\\]\\u@\\h\\\[\\033\[00m\\\]:\\\[\\033\[01;34m\\\]\\w\\\[\\033\[00m\\\]\\\$ '\''/PS1='\''\${debian_chroot:+(\$debian_chroot)}\\\[\\033\[01;37m\\\]\\u@\\\[\\033\[01;31m\\\]docker-\\h\\\[\\033\[00m\\\]:\\\[\\033\[01;35m\\\]\\w\\\[\\033\[00m\\\]\\\$ '\''/g' /root/.bashrc \
&& sed -i 's/alias ls='\''ls --color=auto'\''/alias ls='\''ls -asiF --color=auto'\''/g' /root/.bashrc \
&& echo "alias xterm='xterm -fa \"Monospace\" -fs 10 -fg white -bg black'" >> /root/.bashrc \
&& echo 'export LD_LIBRARY_PATH=/opt/tritonserver/lib:/usr/local/cuda-12.6/lib64:/usr/local/cuda-12.6/targets/aarch64-linux/lib:/usr/local/lib' >> /root/.bashrc \
&& echo 'export LANG=en_US.UTF-8' >> /root/.bashrc \
&& echo 'export NO_AT_BRIDGE=1' >> /root/.bashrc \
&& echo 'export PATH=$PATH:/usr/src/tensorrt/bin' >> /root/.bashrc \
&& echo "complete -d cd" >> /root/.bashrc


#########################################
